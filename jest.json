{
  "coverageReporters": [
    "html",
    "lcov",
    "text-summary"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "setupFilesAfterEnv": ["./test/index.js"],
  "testRegex": "test/.*\\.spec.js",
  "verbose": true
}
